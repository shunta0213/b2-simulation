{
	"name": "LaTeX",
	"build": {
		"context": ".",
		"dockerfile": "Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"James-Yu.latex-workshop",
				"mhutchie.git-graph",
				"ms-azuretools.vscode-docker",
				"taichi.vscode-textlint"
			]
		}
	},
	"workspaceMount": "source=${localWorkspaceFolder},target=/workdir,type=bind",
	"workspaceFolder": "/workdir"
}